matrix.MatrixBuilder<;R extends matrix.Size, C extends matrix.Size> {
  random() row(R row) col(C col) matrix.Matrix<R, C>;
}

matrix.Matrix<R extends matrix.Size, C extends matrix.Size; NEW_C extends matrix.Size> {
  plus(matrix.Matrix<R, C> matrix) matrix.Matrix<R, C>;
  mult(matrix.Matrix<C, NEW_C> matrix) matrix.Matrix<R, NEW_C>;
}
